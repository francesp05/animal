two_digit = input("Type a two digit number ")
# two_numbers = input("What numbers do you want to add?")

deez = (two_digit[0])
# print("+")
nutz = (two_digit[1])
# print("=9")
# print(deez+nutz)

print(int(deez)**int(nutz))

